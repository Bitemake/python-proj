ride_type = "Black"
credits = 27
ride_price = 0
final_price = 0
if ride_type == "UberX":
    ride_price = 21.4
elif ride_type == "Black":
    ride_price = 39.2
else:
    ride_price = 20.1
print("Ride price:")
print(ride_price)
if credits > 0:
    final_price = ride_price - credits
print("Final price:")
print(final_price)